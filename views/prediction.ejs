                    <p class="h3  text-center font-weight-bold mt-3 result" id="charCount">0</p>
                  </div>
                </div>
              </div>
              <div class="col-md-4 col-sm-12 transparent ">
                <div class="card heure-card ">
                  <div class="card-body d-flex flex-column align-items-between">
                    <p class=" d-flex justify-content-between"><span>Similarité Maximale</span><i class="bi bi-arrow-up"></i></p>
                    <p class="h3  text-center font-weight-bold mt-3 maxSimilarity" id="wordCount">0</p>
                  </div>
                </div>
              </div>
              <div class="col-md-4 col-sm-12 transparent ">
                <div class="card heure-card ">
                  <div class="card-body d-flex flex-column align-items-between">
                    <p class=" d-flex justify-content-between"><span>Similarité Minimale</span><i class="bi bi-arrow-down"></i></p>
                    <p class="h3  text-center font-weight-bold mt-3 minSimilarity" id="dataSetSize">0</p>
                  </div>
                </div>
              </div>
            </div>
          <div class="row mt-2 d-none" id="knnChart">
