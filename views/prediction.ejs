        <div class=" container-fluid main-panel" id=main-panel-home >
          <div class="content-wrapper" id="main-panel-wrapper">
         <!--First row start-->
            <div class="row ">
              <!--people card container  start-->
              <div class=" col m-lg-0  ">
                <form>
                  <div class="form-group mb-2">
                    <label for="usDescription" class="font-weight-light PageTitle">Description de l'user story</label>
                    <input type="text" class="form-control" id="usDescription" aria-describedby="emailHelp" placeholder="Veuillez fournir la description de votre nouvelle user story">
                  </div>  
                 
                
                  <p class="font-weight-light PageTitle">Emplacement de l'équipe de développement</p>
                  <div class="d-flex justify-content-around">
                  <div class="form-check form-check-inline">
                    <input class="form-check-input" type="radio" name="inlineRadioOptions" id="local" value="local" checked>
                    <label class="form-check-label" for="local">En présentiel(par défaut)</label>
                  </div>
                  <div class="form-check form-check-inline">
                    <input class="form-check-input" type="radio" name="inlineRadioOptions" id="remote" value="remote">
                    <label class="form-check-label" for="remote">En remote</label>
                  </div>
                </div>
                <p class="font-weight-light PageTitle mt-2">Taille de l'équipe de développement</p>
                <input type="number" class="form-control" id="teamStrength" min="0" max="150" placeholder="Veuillez specifier la taille de l'equipe de développement">
                  <p class="mt-2">Pour un meilleur résultat :</p>          
                  <div class="alert alert-success" role="alert">
                   Rédiger la description de l'user story en anglais afin d’éviter la réduction de la valeur du texte après la traduction                  
                   </div>
                  <div class="col-12 d-flex justify-content-center">
                    <button type="button" class="btn btn-primary rounded " onclick="estimateStoryPoints()">Estimer <span><i class="bi bi-question-lg"></i></span></button>
                  </div>
                </form>
            </div>
            <!--people card container end -->
            <!--infos card start-->
                <div class="col-6 info-group-container ">
                  <!--info group start-->
                  <div class="row p-0 m-0 d-flex flex-column align-items-stretch">
                    <div class="row">
                    <!--date card start -->
                    <div class="col-6 transparent ">
                      <div class="card heure-card ">
                        <div class="card-body d-flex flex-column align-items-between">
                          <p class=" d-flex justify-content-between"><span>Nombre de caractères </span><i class="bi bi-calculator"></i></p>
                          <p class="h3  text-center font-weight-bold mt-3 charCount" id="charCount">0</p>
                        </div>
                      </div>
                    </div>
                    <!--date card end-->
                    <!--heure card start-->
                    <div class="col-6 stretch-card">
                      <div class="card date-card ">
                        <div class="card-body d-flex flex-column align-items-between">
                        <p class=" d-flex justify-content-between "><span>Nombre de mots </span><i class="bi bi-calculator"></i> </p>
                        <p class="h3  text-center font-weight-bold mt-3 wordCount" id="wordCount">0</p>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <!--heure card end-->
                    <!--taches card start-->
                    <div class="col-6 stretch-card transparent">
                    <div class="card taches-card">
                      <div class="card-body d-flex flex-column align-items-between">
                        <p class=" d-flex justify-content-between"><span>Taille du dataset</span><i class="bi bi-bounding-box"></i> </p>
                        <p class="h3  text-center font-weight-bold mt-3 dataSetSize" id="dataSetSize">0</p>
                      </div>
                    </div>
                  </div>
                  <!--taches card end-->
                  <!--projets card start-->
                    <div class="col-6 stretch-card transparent">
                      <div class="card card-light-danger projets-card">
                        <div class="card-body d-flex flex-column align-items-between">
                          <p class=" d-flex justify-content-between"><span>Similarité moyenne</span><i class="bi bi-percent"></i></p>
                          <p class="h3  text-center font-weight-bold mt-3 similarityAverage" id="similarityAverage">0</p>
                        </div>
                      </div>
                    </div>
                    <!--projets card end-->
                  </div>
                  </div>
                  <!--info group end -->
                </div>
            <!--infos card end -->
            </div>
            <div class="row mt-2">
              <div class="col-md-4 col-sm-12 transparent ">
                <div class="card heure-card ">
                  <div class="card-body d-flex flex-column align-items-between">
