        <div class=" container-fluid main-panel" id=main-panel-home >
          <div class="content-wrapper" id="main-panel-wrapper">
            <div class="d-none " id="projectIdentifier"><%=projet._id%></div>
            <!--First row start-->
            <div class="row d-flex justify-content-between">
                <div class="page-title"><h4>Ajouter un membre </h4></div>
            </div>
            <div class="row mt-2 mx-3 d-flex justify-content-center p-2 shadow-sm" id="team_content_panel">
                <input class="form-control form-control-lg col-sm-9" type="text" id="searchField" placeholder="Écrire ici ...">
                <button type="button" class="btn btn-outline-violet ml-2" onclick="launchSearch()"> <i class="bi bi-search mr-2"></i>Rechercher</button>
            </div>
            <div class="row mt-2 d-flex justify-content-around">
              <%for (var i=0;i<developers.length;i++){%>
                <div class="card   mb-1 p-2 team-member-card shadow-sm" style="width: 18rem;">
                  <div class="d-flex justify-content-center">
                       <img src="<%=developers[i].photo%>" class="card-img-top team-card-img" alt="...">
                  </div>
                  <div class="card-body d-flex justify-content-center flex-column align-items-center">
                    <h5 class="card-title developerName"><%=developers[i].nom%> <%=developers[i].prenom%></h5>
                    <div class=" d-flex flex-column">
                      <a  class="btn   viewProfileBtn text-light mt-2 mx-1 " href="/users/profile/view/<%=developers[i]._id%>">Voir le Profil<i class="bi bi-tools ml-1"></i></a>
                      <button type="button" class="btn btn-warning  addMemberToTeam text-light mt-2"  id="<%=developers[i]._id%>" onclick="addMemberToTeam(event)">Ajouter à L'équipe<i class="bi bi-person-plus ml-1" id="<%=developers[i]._id%>" onclick="addMemberToTeam(event)"></i></button>
